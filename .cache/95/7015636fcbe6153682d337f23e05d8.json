e":"components/Blocks.js","original":{"line":27,"column":6}},{"generated":{"line":126,"column":9},"source":"components/Blocks.js","original":{"line":26,"column":4}},{"generated":{"line":127,"column":0},"source":"components/Blocks.js","original":{"line":54,"column":3}},{"name":"Component","generated":{"line":131,"column":2},"source":"components/Blocks.js","original":{"line":6,"column":21}},{"name":"Blocks","generated":{"line":133,"column":15},"source":"components/Blocks.js","original":{"line":57,"column":15}}],"sources":{"components/Blocks.js":"import React, { Component } from 'react';\nimport { Button } from 'react-bootstrap';\nimport { Link } from 'react-router-dom';\nimport Block from './Block';\n\nclass Blocks extends Component {\n  state = { blocks: [], paginatedId: 1, blocksLength: 0 };\n\n  componentDidMount() {\n    fetch(`${document.location.origin}/api/blocks/length`)\n      .then(response => response.json())\n      .then(json => this.setState({ blocksLength: json }));\n\n    this.fetchPaginatedBlocks(this.state.paginatedId)();\n  }\n\n  fetchPaginatedBlocks = paginatedId => () => {\n    fetch(`${document.location.origin}/api/blocks/${paginatedId}`)\n      .then(response => response.json())\n      .then(json => this.setState({ blocks: json }));\n  }\n\n  render() {\n    console.log('this.state', this.state);\n      \n    return (\n      <div>\n        <div><Link to='/'>Home</Link></div>\n        <h3>Blocks</h3>\n        <div>\n          {\n            [...Array(Math.ceil(this.state.blocksLength/5)).keys()].map(key => {\n              const paginatedId = key+1;\n\n              return (\n                <span key={key} onClick={this.fetchPaginatedBlocks(paginatedId)}>\n                  <Button bsstyle=\"small\" bsstyle=\"danger\">\n                    {paginatedId}\n                  </Button>{' '}\n                </span>\n              )\n            })\n          }\n        </div>\n        {\n          this.state.blocks.map(block => {\n            return (\n              <Block key={block.hash} block={block} />\n            );\n          })\n        }\n      </div>\n    );\n  }\n}\n\nexport default Blocks;"},"lineCount":null}},"error":null,"hash":"4c1411aad3617ea57893e87843bc0248","cacheData":{"env":{}}}